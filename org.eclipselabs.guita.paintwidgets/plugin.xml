<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="org.eclipselabs.guita.paint"
               label="Paint"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.eclipselabs.guita.paintwidgets.PaintCommand"
            id="org.eclipselabs.guita.paint"
            name="Paint">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.eclipselabs.guita.paintwidgets.view.ViewTable"
            id="org.eclipselabs.guita.paintwidgets.ViewTable"
            name="ViewTable"
            restorable="true">
      </view>
   </extension>

</plugin>
